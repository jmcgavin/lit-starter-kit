{
  "name": "lit-element-starter-kit",
  "version": "0.0.0",
  "author": "Jordan McGavin",
  "description": "A web app starter kit using LitElement + Redux",
  "license": "ISC",
  "repository": "https://github.com/jmcgavin/lit-element-starter-kit",
  "scripts": {
